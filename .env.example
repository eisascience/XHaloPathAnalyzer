# Environment variables for XHalo Path Analyzer
# Copy this file to .env and update with your values

# Halo API Configuration
HALO_API_URL=https://your-halo-instance/graphql
HALO_API_KEY=your-api-key-here

# MedSAM Model Configuration
MEDSAM_MODEL_PATH=/app/models/medsam.pth
DEVICE=cpu

# Application Settings
LOG_LEVEL=INFO
MAX_UPLOAD_SIZE=200
